import Calculator

def test_addition():
    assert Calculator.Add(1, 2) == 3

def test_subtraction():
    assert Calculator.Subtract(5, 3) == 2

def test_multiplication():
    assert Calculator.Multiply(2, 3) == 6

def test_division():
    assert Calculator.Divide(10, 2) == 5

def test_exponent():
    assert Calculator.Exponent(2, 3) == 8
