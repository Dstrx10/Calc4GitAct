# test_calculator.py

from Calculator import Add, Subtract, Multiply, Divide, Exponent

def test_add():
    assert Add(1, 2) == 3

def test_subtract():
    assert Subtract(5, 3) == 2

def test_multiply():
    assert Multiply(2, 3) == 6

def test_divide():
    assert Divide(10, 2) == 5

def test_exponent():
    assert Exponent(2, 3) == 8
